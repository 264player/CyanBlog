@model CyanBlog.Models.Message

@{
    ViewData["Title"] = "留言详情";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="layui-col-md8 layui-col-md-offset2 layui-panel glass-container" style="padding:32px;margin-bottom:32px; min-height: 100%;height: 100%;">
    <div>
        <h4>Message</h4>
        <hr />
        <dl class="layui-row">
            <dt class="layui-col-md2">
                @Html.DisplayNameFor(model => model.Content)
            </dt>
            <dd class="layui-col-md10">
                @Html.DisplayFor(model => model.Content)
            </dd>
            <dt class="layui-col-md2">
                @Html.DisplayNameFor(model => model.CreateTime)
            </dt>
            <dd class="layui-col-md10">
                @Html.DisplayFor(model => model.CreateTime)
            </dd>
            <dt class="layui-col-md2">
                @Html.DisplayNameFor(model => model.UserId)
            </dt>
            <dd class="layui-col-md10">
                @Html.DisplayFor(model => model.UserId)
            </dd>
            <dt class="layui-col-md2">
                @Html.DisplayNameFor(model => model.ManagerId)
            </dt>
            <dd class="layui-col-md2">
                @Html.DisplayFor(model => model.ManagerId)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model?.MessageId">编辑</a> |
        <a asp-action="ViewList">返回列表</a>
    </div>
</div>

